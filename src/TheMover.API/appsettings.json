{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "http": {
        "Url": "https://localhost:5000",
        "Protocols": "Http1",
        "Certificate": {
          "Path": "/letsencrypt/api.themover.site/certificate.crt",
          "KeyPath": "/letsencrypt/api.themover.site/privatekey.key"
        }
      },
      "gRPC": {
        "Url": "https://localhost:5001",
        "Protocols": "Http2",
        "Certificate": {
          "Path": "/letsencrypt/grpc.themover.site/certificate.crt",
          "KeyPath": "/letsencrypt/grpc.themover.site/privatekey.key"
        }
      }
    }
  },
  "AllowedHosts": "*"
}
